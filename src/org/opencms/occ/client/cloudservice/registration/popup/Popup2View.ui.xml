<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:b="urn:import:org.apache.tomcat.util.buf">

	<g:PopupPanel glassEnabled="true">
		<g:HTMLPanel styleName="popup">
			<b>Registration has been successful!</b> <br/>
			<b>Confirmation has been sent to your e-mail.</b>
			<br />
			<g:Button text="OK" ui:field="okButten" styleName="popupButton"
				focus="true" />
		</g:HTMLPanel>

	</g:PopupPanel>
</ui:UiBinder>
